<% include partials/top %>
<main>
<h1>WELCOME</h1>
<p>To the best website ever all about</p>
<h2><% for (var i = 0; i < title.length; i++) { %><span class="ft"><%= title[i] %></span><% } %></h2>
<section>
  <header><h2>Cat of the day!</h2></header>
  <div class="content">
    <h3><%=catname%></h3>
    <figure>
      <img src="/img/<%=cat.thumbnail%>"/>
    </figure>
    <a href="/cat/<%=catname%>">See this cat!</a>
  </div>
</section>
<section>
  <header><h2>Cat database</h2></header>
  <div class="content">
    <a href="cat">Come look at all the cats!</a>
  </div>
</section>
</main>
